{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "Path": "/C:/Users/Casper/Desktop/GitHub Projects/ElasticsearchApp/src/ElasticsearchAPI/ElasticsearchAPI/Logs/APILog-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "ElasticSearch": {
    "ApplicationName": "ElasticApp",
    "DefaultIndex": "Index-{0:yyyy.MM.dd}",
    "ElasticUrl": "http://localhost:9200/",
    "UserName": "",
    "Password": ""
  }
}